@model IEnumerable<WebBanHang.Models.CategoriesModel>

@foreach (var item in Model)
{
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">
                <a asp-action="CategoriesIndex" asp-controller="Categories" asp-route-id="@item.CategoryID">
                    @item.CategoryName
                </a>
            </h4>
        </div>
    </div>
}
